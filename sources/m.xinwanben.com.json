{
    "name": "新完本神站",
    "url": "m.xinwanben.com",
    "version": 103,
    "search": {
        "url": "http://m.xinwanben.com/s.php@post->s=${key}&type=articlename",
        "charset": "gbk",
        "list": ".sone",
        "name": ".sone > a",
        "author": ".sone > span",
        "detail": ".sone > a"
    },
    "detail": {
        "cover": ".pic > img",
        "summary": ".intro > p",
        "update": ".infotype > p:nth-child(3)@match->(?<=时间：)(.+)",
        "lastChapter": ".infotype > p:nth-child(4) > a",
        "catalog": ".gochapter > a@replace->m.xinwanben->www.xinwanben"
    },
    "catalog": {
        "list": "ul.chapters > li",
        "orderBy": 0,
        "name": "a",
        "chapter": "a"
    },
    "chapter": {
        "content": "#articlecontent"
    }
}
